{
	"name": "atums.status",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "bun run dev:api & bun run dev:web",
		"dev:api": "bun run src/api/index.ts",
		"dev:web": "bunx vite dev",
		"build": "bunx vite build",
		"preview": "bunx vite preview",
		"start": "bun run src/api/index.ts",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"cleanup": "rm -rf logs node_modules bun.lock"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.0",
		"@eslint/js": "^9.39.1",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.48.5",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@types/bun": "^1.3.3",
		"eslint": "^9.39.1",
		"eslint-plugin-svelte": "^3.13.1",
		"globals": "^16.5.0",
		"svelte": "^5.43.8",
		"svelte-check": "^4.3.4",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.48.1",
		"vite": "^7.2.2"
	},
	"dependencies": {
		"@atums/echo": "^2.0.3"
	}
}
